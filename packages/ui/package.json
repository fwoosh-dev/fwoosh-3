{
  "name": "@fwoosh/ui",
  "description": "",
  "version": "0.1.0",
  "sideEffects": false,
  "scripts": {
    "build": "tshy",
    "clean": "rimraf dist .tshy .turbo out",
    "gen-colors": "tsx scripts/gen-colors.ts"
  },
  "keywords": [],
  "author": "Andrew Lisowski",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-context": "^1.0.1",
    "@stylexjs/open-props": "^0.6.1",
    "@stylexjs/stylex": "^0.6.1",
    "lucide-react": "^0.395.0",
    "react-aria": "^3.33.1",
    "react-aria-components": "^1.2.1",
    "remark": "^15.0.1",
    "remark-emoji": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-unwrap-images": "^4.0.0",
    "shiki": "^1.10.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@fwoosh/config-typescript": "workspace:*",
    "@fwoosh/types": "workspace:*",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.14.2",
    "@types/react": "18.3.1",
    "dedent": "^1.5.3",
    "react": "19.0.0-rc.0",
    "react-server-dom-webpack": "19.0.0-rc.0",
    "rimraf": "^5.0.7",
    "tshy": "^1.15.1",
    "tsx": "^4.15.5"
  },
  "tshy": {
    "dialects": [
      "esm"
    ],
    "exports": [
      "src/index.ts",
      "src/app.tsx",
      "src/components/**",
      "src/theme/*.stylex.ts",
      "src/utils/*.ts"
    ]
  },
  "exports": {
    "./app": {
      "import": {
        "source": "./src/app.tsx",
        "types": "./dist/esm/app.d.ts",
        "default": "./dist/esm/app.js"
      }
    },
    "./components/Button": {
      "import": {
        "source": "./src/components/Button.tsx",
        "types": "./dist/esm/components/Button.d.ts",
        "default": "./dist/esm/components/Button.js"
      }
    },
    "./components/FocusRing": {
      "import": {
        "source": "./src/components/FocusRing.tsx",
        "types": "./dist/esm/components/FocusRing.d.ts",
        "default": "./dist/esm/components/FocusRing.js"
      }
    },
    "./components/HStack": {
      "import": {
        "source": "./src/components/HStack.tsx",
        "types": "./dist/esm/components/HStack.d.ts",
        "default": "./dist/esm/components/HStack.js"
      }
    },
    "./components/IconButton": {
      "import": {
        "source": "./src/components/IconButton.tsx",
        "types": "./dist/esm/components/IconButton.d.ts",
        "default": "./dist/esm/components/IconButton.js"
      }
    },
    "./components/InlineCode": {
      "import": {
        "source": "./src/components/InlineCode.tsx",
        "types": "./dist/esm/components/InlineCode.d.ts",
        "default": "./dist/esm/components/InlineCode.js"
      }
    },
    "./components/Label": {
      "import": {
        "source": "./src/components/Label.tsx",
        "types": "./dist/esm/components/Label.d.ts",
        "default": "./dist/esm/components/Label.js"
      }
    },
    "./components/Markdown": {
      "import": {
        "source": "./src/components/Markdown.tsx",
        "types": "./dist/esm/components/Markdown.d.ts",
        "default": "./dist/esm/components/Markdown.js"
      }
    },
    "./components/Popover": {
      "import": {
        "source": "./src/components/Popover.tsx",
        "types": "./dist/esm/components/Popover.d.ts",
        "default": "./dist/esm/components/Popover.js"
      }
    },
    "./components/Select": {
      "import": {
        "source": "./src/components/Select.tsx",
        "types": "./dist/esm/components/Select.d.ts",
        "default": "./dist/esm/components/Select.js"
      }
    },
    "./components/Spinner": {
      "import": {
        "source": "./src/components/Spinner.tsx",
        "types": "./dist/esm/components/Spinner.d.ts",
        "default": "./dist/esm/components/Spinner.js"
      }
    },
    "./components/Table": {
      "import": {
        "source": "./src/components/Table.tsx",
        "types": "./dist/esm/components/Table.d.ts",
        "default": "./dist/esm/components/Table.js"
      }
    },
    "./components/Tabs": {
      "import": {
        "source": "./src/components/Tabs.tsx",
        "types": "./dist/esm/components/Tabs.d.ts",
        "default": "./dist/esm/components/Tabs.js"
      }
    },
    "./components/TextField": {
      "import": {
