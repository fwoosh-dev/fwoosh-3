{
  "name": "@fwoosh/plugin-a11y",
  "description": "Tools to test the accessibility of your stories",
  "version": "0.1.0",
  "scripts": {
    "build": "tshy",
    "clean": "rimraf dist .tshy"
  },
  "keywords": [],
