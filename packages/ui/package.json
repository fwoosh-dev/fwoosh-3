{
  "name": "@fwoosh/ui",
  "description": "",
  "version": "0.1.0",
  "sideEffects": false,
  "scripts": {
    "build": "tshy",
    "clean": "rimraf dist .tshy",
    "gen-colors": "tsx scripts/gen-colors.ts"
  },
  "keywords": [],
  "author": "Andrew Lisowski",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-context": "^1.0.1",
    "@stylexjs/open-props": "^0.6.1",
    "@stylexjs/stylex": "^0.6.1",
    "lucide-react": "^0.395.0",
    "mdast": "^3.0.0",
    "react-aria": "^3.33.1",
    "react-aria-components": "^1.2.1",
    "remark-emoji": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-unwrap-images": "^4.0.0",
    "shiki": "^1.7.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@fwoosh/config-typescript": "workspace:*",
    "@fwoosh/types": "workspace:*",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.14.2",
    "@types/react": "18.3.1",
    "dedent": "^1.5.3",
    "react": "19.0.0-beta-e7d213dfb0-20240507",
    "react-server-dom-webpack": "19.0.0-beta-e7d213dfb0-20240507",
    "rimraf": "^5.0.7",
    "tshy": "^1.15.1",
    "tsx": "^4.15.5"
  },
  "tshy": {
    "dialects": [
      "esm"
    ],
    "exports": {
      ".": "./src/index.ts",
      "./components": "./src/components/index.ts",
      "./tokens.stylex": "./src/theme/tokens.stylex.ts",
      "./theme.stylex": "./src/theme/theme.stylex.ts",
      "./app": "./src/app/app.tsx",
      "./highlight-syntax": "./src/utils/highlightSyntax.ts"
    }
  },
  "exports": {
    ".": {
      "import": {
        "source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./components": {
      "import": {
        "source": "./src/components/index.ts",
        "types": "./dist/esm/components/index.d.ts",
        "default": "./dist/esm/components/index.js"
      }
    },
    "./tokens.stylex": {
      "import": {
        "source": "./src/theme/tokens.stylex.ts",
        "types": "./dist/esm/theme/tokens.stylex.d.ts",
        "default": "./dist/esm/theme/tokens.stylex.js"
      }
    },
    "./theme.stylex": {
      "import": {
        "source": "./src/theme/theme.stylex.ts",
        "types": "./dist/esm/theme/theme.stylex.d.ts",
        "default": "./dist/esm/theme/theme.stylex.js"
      }
    },
    "./app": {
      "import": {
        "source": "./src/app/app.tsx",
        "types": "./dist/esm/app/app.d.ts",
        "default": "./dist/esm/app/app.js"
      }
    },
    "./highlight-syntax": {
      "import": {
        "source": "./src/utils/highlightSyntax.ts",
        "types": "./dist/esm/utils/highlightSyntax.d.ts",
        "default": "./dist/esm/utils/highlightSyntax.js"
      }
    }
  },
  "type": "module"
}
