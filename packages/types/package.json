{
  "name": "@fwoosh/types",
  "description": "Shared types for fwoosh",
  "version": "0.1.0",
  "scripts": {
    "build": "tshy",
    "clean": "rimraf dist .tshy .turbo out"
  },
  "keywords": [],
  "author": "Andrew Lisowski",
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.4",
    "shiki": "^1.10.0",
    "type-fest": "^4.20.1"
  },
  "devDependencies": {
    "@fwoosh/config-typescript": "workspace:*",
    "@types/invariant": "^2.2.37",
    "@types/node": "^20.14.2",
    "rimraf": "^5.0.7",
    "tshy": "^1.15.1"
  },
  "tshy": {
    "dialects": [
      "esm"
    ],
    "exports": {
      ".": "./src/index.ts",
      "./globals": "./globals.d.ts",
      "./resolve-file": "./src/resolveFile.ts"
    }
  },
  "exports": {
    ".": {
      "import": {
        "source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./globals": "./globals.d.ts",
    "./resolve-file": {
      "import": {
        "source": "./src/resolveFile.ts",
        "types": "./dist/esm/resolveFile.d.ts",
        "default": "./dist/esm/resolveFile.js"
      }
    }
  },
  "type": "module"
}
