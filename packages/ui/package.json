        "default": "./dist/esm/components/Tooltip.js"
      }
    },
    ".": {
      "import": {
        "source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./theme/theme.stylex": {
      "import": {
        "source": "./src/theme/theme.stylex.ts",
        "types": "./dist/esm/theme/theme.stylex.d.ts",
        "default": "./dist/esm/theme/theme.stylex.js"
      }
    },
    "./theme/tokens.stylex": {
      "import": {
        "source": "./src/theme/tokens.stylex.ts",
        "types": "./dist/esm/theme/tokens.stylex.d.ts",
        "default": "./dist/esm/theme/tokens.stylex.js"
      }
    },
    "./utils/highlightSyntax": {
      "import": {
        "source": "./src/utils/highlightSyntax.ts",
        "types": "./dist/esm/utils/highlightSyntax.d.ts",
        "default": "./dist/esm/utils/highlightSyntax.js"
      }
    },
    "./package.json": "./package.json"
  },
  "type": "module"
